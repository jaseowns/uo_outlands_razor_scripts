## Script: Bapeth's Modern Dungeon Picker
## Created by: BAZZACUNT#9523
#############################################

//***Bapeths Modern Dungeon Picker***

//"***Recommended Build***"

//Detect Hidden - 120
//Lockpicking ----120
//Stealth ------- 120
//Hiding -------- 100
//Magery -------- 100
//Camping -------- 80
//Spirit Speak --- 80

//Harvest or Fortune Aspect
//Special/Rare loot Chance Links
//Chest Success Chances/Preogress Links

//"***READ THIS***"

//It is reccomended to have a
//"blessed and aspected" "armored magical wizard hat" in your pack
//This is for the stealth part of the script
//It will quick swap your hat and reduce your stamina
//This will prevent you from pushing through mobiles while stealthing

//Toggle "WARMODE" ON and you will not hide when picking chests
//This is for picking in "BOSS ROOMS" where a custodian is present

//This script uses razor "auto-queue object delay" setting
//Make sure this setting is turned ON in the razor options tab

//This script uses two cooldowns called "Hiding" and "Stealth"
//You must create these in the in-game options menu under Cooldowns
//Leave at 0 or set them to 10.4 seconds for use outside the script

//See "line 47" if you would like to custom name your earth elemental
//Default is "TreasureGolem"

//Notes on error : 
//Screen transitions and teleporting far distances
//Will on occasion error the script 
//This is a bug in razor
//Simply play the script again if this happens

if skill "Spirit Speak" >= 80 and findtype "an earth elemental" ground -1 -1 1 as Summon
    if noto Summon = "friend"
        //Name your summon if you like (no spaces must be unique)
        rename Summon "TreasureGolem"
    endif
endif

if not find "bwiz" backpack and findtype 5912 backpack as bwiz
    @setvar "bwiz" bwiz
endif

if not find "bmain" self and findlayer self head as hat
    @setvar "bmain" hat
endif

if not findtype "lockpicks"
    overhead "I will need some Lockpicks..." 38
    stop
endif

if not findtype "drill"
    overhead "I will need a Trap Removal Tool..." 37
    stop
endif

if timerexists ots
    removetimer ots
endif

if timerexists bchestopen
    removetimer bchestopen
endif

if skill "Stealth" >= 80 and hidden and stam = maxstam and findtype 5912 backpack
    while not queued
        if findlayer self head as hat
            getlabel hat desc
            if "magical" in desc
                dclick "bmain"
            else 
                dclick "bwiz"
                dclick "bmain"
            endif
        endif
        break
    endwhile
endif

if not findlayer self head
    dclick "bmain"
endif

if not gumpexists 736038070
    @clearignore 
    for 16
        if index > 0 and findtype 3651|3649|3648|3650|3708|2475 ground -1 -1 index as chest
            getlabel chest desc
            if "lock" in desc
                overhead "{{desc}}" 2085 chest
                break
            else 
                overhead "{{desc}}" 1779 chest
                @ignore chest
            endif
        endif
    endfor
endif

if gumpexists 736038070
    gumpclose 736038070
endif

if findtype 'metal chest' ground -1 -1 2 as bchest and not targetexists 
    clearsysmsg 
    dclick bchest
    @setvar "bchest" lastobject
    waitforgump 736038070 500
    if gumpexists 736038070 and not ingump "Cleared Of Traps"
        gumpresponse 7
        waitforgump 736038070 500
        pause 350
        if insysmsg "You attempt to remove traps"
            if not cooldown "Hiding" and not hidden and not warmode
                if followers >= 1
                    say "all patrol" 45
                    wft 500
                    target self
                endif
                if skill "Hiding" >= 80
                    warmode 'on'
                    warmode 'off'
                    skill "Hiding"
                    if hidden
                        cooldown "Hiding" 10350
                    endif
                endif
            endif
            overhead "I will make short work of this trap..." 67
            waitforgump 736038070 500
            while gumpexists 736038070
                if not hidden and not cooldown "Hiding" and not warmode
                    warmode 'on'
                    warmode 'off'
                    skill "Hiding"
                    if hidden
                        cooldown "Hiding" 10350
                    endif
                endif
                if hidden and not cooldown "Hiding" and not cooldown "Stealth" and not timerexists ots
                    skill "Stealth"
                    cooldown "Stealth" 10350
                    if not timerexists ots
                        createtimer ots
                    endif
                endif
                if insysmsg  "You have successfully cleared it of traps"
                    break
                endif
                if not findtype 'metal chest' ground -1 -1 2
                    break
                endif
                if insysmsg "Another player"
                    replay
                endif
                if ingump "Cleared Of Traps"
                    break
                endif
                if skill "Stealth" >= 80 and hidden and stam = maxstam and findtype 5912 backpack
                    while not queued
                        if findlayer self head as hat
                            getlabel hat desc
                            if "magical" in desc
                                dclick "bmain"
                            else 
                                dclick "bwiz"
                                dclick "bmain"
                            endif
                        endif
                        break
                    endwhile
                endif
            endwhile
            overhead "That should do it..." 67
        endif
    endif
    if gumpexists 736038070 and ingump "Cleared Of Traps"
        gumpresponse 4
        waitforgump 736038070 500
        pause 350
        if insysmsg "You attempt to pick the lock"
            if not cooldown "Hiding" and not hidden and not warmode 
                if followers >= 1
                    say "all patrol" 45
                    wft 500
                    target self
                endif
                if skill "Hiding" >= 80
                    warmode 'on'
                    warmode 'off'
                    skill "Hiding"
                    if hidden
                        cooldown "Hiding" 10350
                    endif
                endif
            endif
            overhead "This lock wont keep me out..." 87
            waitforgump 736038070 500
            while gumpexists 736038070
                if not hidden and not cooldown "Hiding" and not warmode
                    warmode 'on'
                    warmode 'off'
                    skill "Hiding"
                    if hidden
                        cooldown "Hiding" 10350
                    endif
                endif
                if hidden and not cooldown "Hiding" and not cooldown "Stealth" and not timerexists ots
                    skill "Stealth"
                    cooldown "Stealth" 10350
                    if not timerexists ots
                        createtimer ots
                    endif
                endif
                if insysmsg  "You successfully pick the lock"
                    break
                endif
                if not findtype 'metal chest' ground -1 -1 2
                    break
                endif
                if insysmsg "Another player"
                    replay
                endif
                if find "bchest" ground -1 -1 2
                    getlabel bchest desc
                    if "lock" in desc
                        //do nothing
                    else 
                        break
                    endif
                endif
                if skill "Stealth" >= 80 and hidden and stam = maxstam and findtype 5912 backpack
                    while not queued
                        if findlayer self head as hat
                            getlabel hat desc
                            if "magical" in desc
                                dclick "bmain"
                            else 
                                dclick "bwiz"
                                dclick "bmain"
                            endif
                        endif
                        break
                    endwhile
                endif
            endwhile
        endif
    endif
endif

if findtype 'wooden chest' ground -1 -1 2 as bchest and not targetexists 
