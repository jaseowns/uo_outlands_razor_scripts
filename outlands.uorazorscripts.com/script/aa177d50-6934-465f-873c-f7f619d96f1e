    overhead "We got our loot bag!" 88
    dclick jootbag
    wait globalTimeout
    @clearignore 
    while findtype "pouch" backpack 0 as item
        getlabel item desc
        if "(0 items" in desc
            // do nothing
        else
            dclick item
            wait 200
        endif
        @ignore item
    endwhile
    clearignore
