                getlabel mySpellbook mySpellbooklabel            
                if "Aspect" in mySpellbooklabel
                    break
                elseif "unidentified" in mySpellbooklabel
                    ignore mySpellbook
                elseif "[blessed" in mySpellbooklabel
                    ignore mySpellbook
