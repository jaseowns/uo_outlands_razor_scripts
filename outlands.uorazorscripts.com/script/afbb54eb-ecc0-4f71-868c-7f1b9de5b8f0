                overhead "This tool is worn out!" 805
            endif
            if insysmsg "fail"
                //do nothing
            elseif insysmsg "You pick the lock!"
                //do nothing
            elseif insysmsg  "You must remain stationary"
                //do nothing
            else
                warmode "off"
                getlabel backpack ping
                break
            endif
        endfor
     else 
         overhead "I am out of lockpicks!" 38
         warmode "off"
