endif

    

   

 
    if ingump 'Page 15' 1274830955
        overhead '15 pages'
    clearsysmsg
    wait 250 
    gumpresponse 10 1274830955
    waitforgump 1274830955
    gumpresponse 11 1274830955
    waitforgump 1274830955
    gumpresponse 12 1274830955
    waitforgump 1274830955
    gumpresponse 13 1274830955
    waitforgump 1274830955
    gumpresponse 14 1274830955
    waitforgump 1274830955
    gumpresponse 15 1274830955
    waitforgump 1274830955
    gumpresponse 16 1274830955
