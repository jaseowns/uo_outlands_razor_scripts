createlist JewelsTypes

@pushlist 'JewelsTypes' 3862 //amethyst
@pushlist 'JewelsTypes' 3856 //emerald
@pushlist 'JewelsTypes' 3885 //tourmaline
@pushlist 'JewelsTypes' 3865 //sapphire
@pushlist 'JewelsTypes' 3861 //citrine
@pushlist 'JewelsTypes' 3877 //amber
@pushlist 'JewelsTypes' 3878 //diamond
@pushlist 'JewelsTypes' 3859 //ruby
@pushlist 'JewelsTypes' 3873 //starsapphire

foreach x in ItemTypes
  while findtype x backpack as found
    hotkey 'set last target'
    target found
    @setvar myVariable lasttarget
    wait 200
    getlabel 'myVariable' myLabel
    if 'unidentified' in myLabel
    lift myVariable 1
    drop junkBag  -1 -1 0
    elseif 'blessed' in myLabel
        #ignore blessed and continue
        @ignore myVariable
    elseif 'exceptional' in myLabel
        @ignore myVariable
    endif
    sysmsg myLabel 85
    wait 1000
  endwhile
endfor

sysmsg 'Unidentified object movement complete'

foreach x in JewelsTypes
  while findtype x backpack as found
    hotkey 'set last target'
    target found
    @setvar myJewelsVariable lasttarget
    wait 200
    lift myJewelsVariable 999
    drop jewelsBag  -1 -1 0
    wait 1000
  endwhile
endfor

sysmsg 'Jewels movement complete'
sysmsg 'All Done'