# Easy Train Discordance by Jaseowns
# UO Outlands
if findtype "lute|lap harp|harp|bamboo flute|tambourine|drum" backpack as item
    dclick item
    wait 650
endif

if not targetexists 
    useskill "Discordance"
    wft 500
endif
if targetexists 
    hotkey "Target Self"
endif
loop