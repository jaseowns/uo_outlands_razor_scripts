#********** CAST SUMMON CREATURE **********# 
        
        elseif summonCreature != 0 and list creatureListy < summonCreature and followers < 5 and diffhits < medDiff and not casting                   
            cast 'Summ. Creature'
        
            while not casting                   
                cast 'Summ. Creature'  
                if targetexists
                    hotkey 'cancel current target'
                endif
            endwhile  
            
            while casting
                if diffhits >= medDiff
                    @setvar! wasInterrupted 1 
