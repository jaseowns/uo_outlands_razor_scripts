                @clearignore
            endif
        
#********** CAST FIRE ELEMENTAL **********# 
        
        elseif fireElementals != 0 and list fireListy < fireElementals and followers < 4 and diffhits < medDiff and not casting                       
            cast 'Fire Elemental'
        
