                        wait 50
                        endwhile
                    endwhile
                    lift thisbook
                    drop container_fullbooks
                    while queued
                        wait 50
                    endwhile
                                else
                                overhead 'Cant find an empty runebook!'
                                break
                                endif
                  if findtype 5356 backpack 2091 as map
                        lift map
                    drop container_maps
                    while queued
                        wait 50
                    endwhile
                    @ignore map
                    else
                        overhead 'Cant find the map, something has gone wrong!'
                        stop
                    endif
                    if findtype 43206 backpack 2750 as link
