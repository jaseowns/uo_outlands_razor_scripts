# This is an automated backup - check out https://outlands.uorazorscripts.com/script/4f78396c-43ee-43ac-b1bd-1e5f6379e823 for latest
# Automation by Jaseowns
## Script: Dabiri's Main Organizer and Reset Script
## Created by: dabiri#0
#############################################

//This one has a bunch of serials for my own chests and tomes but will perhaps be a useful reference for others. 
//It has a check and an optional sub script to unload a shipping crate automatically

overhead 'Organizer 3.0'
clearignore 

//Box for cores, extracts, skillscrolls, orbs, and research mats
@setvar! aspect_box '0x4B3C1B85'
@setvar! gold_box '0x54862019'
dclick '0x568BEAEB'

//Dump resources
overhead 'Dumping resources'
menu 0x49FA6F42 0
waitfortarget
target 'self'



//Drop off loadout
overhead 'Emptying loadout'
menu 0x49F6F1B7 0
waitfortarget
hotkey 'Target Self'



overhead 'Tome dumps'
getlabel backpack pingCheck
menu 0x4C1E8CE8 0
getlabel backpack pingCheck
menu 0x4858E76E 0
getlabel backpack pingCheck
menu 0x4C1EB663 0
getlabel backpack pingCheck
menu 0x4858E468 0
getlabel backpack pingCheck
menu 0x50C115E9 1
getlabel backpack pingCheck
menu 0x5AA01233 0
getlabel backpack pingCheck
menu 0x5AA01728 0
getlabel backpack pingCheck
menu 0x5AA01504 0
getlabel backpack pingCheck
menu 0x5AA00D7C 0
getlabel backpack pingCheck
menu 0x4130BCB7 0
getlabel backpack pingCheck
menu 0x4130BEE5 0
getlabel backpack pingCheck
menu 0x4130CAF5 0
getlabel backpack pingCheck
menu 0x57847BD9 0
getlabel backpack pingCheck

overhead 'Tome dump done'

overhead 'Stack check statements'

//Move gold
overhead 'Dumping gold and dubs'
menu 0x447863D2 0
getlabel backpack pingCheck

//Move marks
overhead 'check marks'
while findtype "colonial marks" backpack as marks
    overhead 'Marks found'
    lift marks 60000
    drop gold_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Move booty
overhead 'check booty'
while findtype "bag of gems" backpack as booty
    overhead 'Booty found'
    lift booty 30
    drop gold_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Move seeds
overhead 'check seeds'
while findtype "seed of renewal" backpack as seeds
    overhead 'Seeds found'
    lift seeds 5
    drop gold_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Move maps
overhead 'check maps'
while findtype "map" backpack as maps
    overhead 'Map found'
    lift maps
    drop gold_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Cores
overhead 'check cores'
while findtype "aspect core" backpack as cores
    overhead 'Core found'
    lift cores
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Extracts
overhead 'check extracts'
while findtype "bottle" backpack as bottles
    overhead 'Extract found'
    lift bottles
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Skill scrolls
overhead 'check skill scrolls'
while findtype "scroll of calling" backpack as scrolls
    overhead 'Scroll found'
    lift scrolls
    //wait minimumWaitForPing
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Orbs
overhead 'check orbs'
while findtype "void orb" backpack as orbs
    overhead 'Orb found'
    lift orbs
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Research Mats
overhead 'check research mats'
while findtype "research materials" backpack as mats
    overhead 'Research mats found'
    lift mats
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Distillations
overhead 'check distillations'
while findtype "flask" backpack as flasks
    overhead 'Distils found'
    lift flasks
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Chromatic Distill
overhead 'check chroma distill'
while findtype "chroma distil" backpack as chromadistils
    overhead 'Chroma distils found'
    lift chromadistils
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Chromatic core
overhead 'check chroma core'
while findtype "chroma core" backpack as chromacores
    overhead 'Chroma core found'
    lift chromacores
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Black goods
overhead 'check black goods'
while findtype "black market goods" backpack as blackgoods
    overhead 'Black goods found'
    lift blackgoods
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

//Move nets
overhead 'check special nets'
clearignore 
while findtype "fishing net" backpack as net
    getlabel net netname
    if "special" in netname
        overhead 'Found a special net'
        lift net 60000
        drop gold_box -1 -1 0
        while queued
            // Waits for lift and drop
        endwhile
    else 
        ignore net
    endif
endwhile

//Soul jars and spores
overhead 'check souls and spores'
while findtype 43423|58196  backpack as soulspores
    overhead 'Souls or spores found'
    lift soulspores 5
    drop aspect_box -1 -1 0
    while queued
        // Waits for lift and drop
    endwhile
endwhile

if findtype 'crate' ground any any  2 as 'shippingcrate'
    script 'UnloadShippingCrate'
else 
    overhead 'Fresh loadout'
    getlabel backpack pingCheck
    menu 0x49F6F1B7 1
endif

getlabel backpack pingCheck

dclick 0x447FA367

getlabel backpack pingCheck

overhead '~~ Organizer done ~~' 18