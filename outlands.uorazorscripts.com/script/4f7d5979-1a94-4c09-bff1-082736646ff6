@clearignore

overhead "... Counting ..." 85

createlist tripartite
clearlist tripartite
createlist bipartite
clearlist bipartite
createlist precious
clearlist precious
while findtype "gem" backpack
    if findtype "gem" backpack as gem
        getlabel gem gemLabel
        if "a tripartite" in gemLabel
            pushlist tripartite gem
        elseif "a bipartite" in gemLabel
            pushlist bipartite gem
        elseif "a precious" in gemLabel
            pushlist precious gem
        endif
        @ignore gem
    endif
endwhile

