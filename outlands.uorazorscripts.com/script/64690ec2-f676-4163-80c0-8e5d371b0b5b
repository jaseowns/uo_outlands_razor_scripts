@clearsysmsg
# Open Grim & Page 1
overhead 'Opening Grim to Page 1'
if not gumpexists '1281623709'
    dclicktype 'wizards grimoire'
    pause 750
endif

while not ingump 'Create Food' 1281623709
    gumpresponse 2 1281623709
endwhile

# Resetting Book
overhead 'Resetting Book'
gumpresponse 4
waitforgump 1281623709 2000
gumpresponse 4
waitforgump 1281623709 2000

overhead 'Reassigning Points'
# Reassigning Points
overhead 'what?!'
foreach 'x' in 'Tier'
    foreach 'y' in 'PageFlip'
        if 'y' = 'Yes'
            gumpresponse 3
            pause 500
        endif
        poplist 'PageFlip' front    
        break
    endfor
    foreach 'z' in 'GumpButton'
        @setvar 'Gump' 'z'
        poplist 'GumpButton' front    
        break
    endfor

    if 'x' = 1
        gumpresponse 'Gump'
        waitforgump 1281623709 2000
    elseif 'x' = 2
        gumpresponse 'Gump'
        waitforgump 1281623709 2000
        gumpresponse 'Gump'    
        waitforgump 1281623709 2000
    elseif 'x' = 3
        gumpresponse 'Gump'
        waitforgump 1281623709 2000
        gumpresponse 'Gump'
        waitforgump 1281623709 2000
        gumpresponse 'Gump'
        waitforgump 1281623709 2000
    endif
    
endfor

if insysmsg 'before you may reset your points distribution.'
    overhead 'Unable to Reset Points!' 33
    @setvar 'Failed' '1'
endif
if insysmsg 'You do not have enough points remaining to upgrade'
    overhead 'Re-Configure Failed!' 33
    @setvar 'Failed' '1'
endif
if 'Failed' = 1
else
    overhead 'Grim Wizard Re-Configure Complete!'
endif