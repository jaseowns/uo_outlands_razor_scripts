overhead "... Counting ..." 85

createlist counterList
clearlist counterList
pushlist counterList 1
pushlist counterList 2
pushlist counterList 3
pushlist counterList 4
pushlist counterList 5
pushlist counterList 6
pushlist counterList 7
pushlist counterList 8
pushlist counterList 9
pushlist counterList 10

@clearignore

createlist tripartite
clearlist tripartite
while findtype "gem" backpack
    if findtype "gem" backpack as gem
        getlabel gem gemLabel
        if "tripartite" in gemLabel
            pushlist tripartite gem
            @ignore gem
        else
            @ignore gem
        endif
    endif
endwhile

while list tripartite < list counterList
    poplist 'counterList' 'back'
endwhile

while list counterList > 1
    poplist 'counterList' 'front'
endwhile

foreach x in counterList
    overhead "Tripartite Time Gem: {{x}}" 15
endfor

clearlist counterList
pushlist counterList 1
