                                sysmsg "add air 1" 11
                                overhead "add air 1" 11 ele
                            else 
                                @setvar! airSecondly ele
                                sysmsg "add air 2" 11
                                overhead "add air 2" 11 ele
                            endif
                            
                            break
                        endif
                    endif
                    @ignore ele
