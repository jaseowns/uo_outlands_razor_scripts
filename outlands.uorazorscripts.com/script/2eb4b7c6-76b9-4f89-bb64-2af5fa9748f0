@clearignore
while findtype 7956 ground any 1 18 as rune
    getlabel rune desc
    if "wait2spin" in desc
        wait 10
    else
        @ignore rune
    endif
endwhile
@clearignore
walk Up
walk North
walk Right
walk East
walk Down
walk South
walk Left
walk West
loop