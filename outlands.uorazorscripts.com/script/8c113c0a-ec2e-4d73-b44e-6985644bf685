                    # Auto Dungeon Gate if PK comes into target range (dungeons only):
                        @setvar! EnableAutoRedDungeonGate 1
                            @setvar! DungeonEscapeTimeLimit 120000
                    # Script will revert to normal if time limit is exceeded without gating
                endif
            endif 
