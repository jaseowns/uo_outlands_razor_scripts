    pushlist jaseowns_AllEmotesList "[Sleep"
    pushlist jaseowns_AllEmotesList "[Yell"
    pushlist jaseowns_AllEmotesList "[Spit"
    pushlist jaseowns_AllEmotesList "[Yes"

    clearsysmsg 

    foreach jaseowns_CheckEmote in jaseowns_AllEmotesList

        say jaseowns_CheckEmote
        getlabel backpack jaseowns_PingCheck
        @setvar! validEmote 1
        // this will wait until we get a valid response
        if insysmsg "You may not use"
                // do nothing
        elseif insysmsg "You have not yet unlocked this emote."
            @setvar! validEmote 0
        endif

        if validEmote = 1
            if not inlist jaseowns_ValidEmotesList jaseowns_CheckEmote
                pushlist jaseowns_ValidEmotesList jaseowns_CheckEmote
            endif
        endif

    endfor

endif


