@pushlist 'ItemTypes' 48407
if findtype 3651|3649|3648|3650|3708|2475 ground -1 -1 2 as chest
  
    foreach x in ItemTypes
        if findtype x chest as item
            @drop backpack
            lift item 11111
            pause 200
            drop backpack -1 -1 0
            pause 200
            ignore item
        endif
    endfor
 
    stop
    
    
endif