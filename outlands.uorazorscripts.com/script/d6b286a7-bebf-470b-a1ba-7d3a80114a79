@clearignore 
if find "bchest" ground -1 -1 2 as bchest
    clearsysmsg 
    while findtype 5899|3842|2594|51321|3838|51318|5901|5905|5903|48407|51302|4248|27611|15296|43166|5359|5981|3836|3985|17087|5356|29030|24434|22336|29036|3843|29025|51098|8826|576|3827|22326|45315|3821|3891|2539|39898|39896|39892|39918|39911|39916|39897|39905|39891|39917|39912|39909|39889|5207|7031|7026|7034|7033|7027|7035|7029|5078|5063|5059|5105|5060|5138|5129|5201|5142|5143|5076|5106|7610|5139|5090|5103|5132|7177|5061|7181|7179|3920|5042|5127|5117|5185|5125|3938|5177|3937|5044|5121|3915|3909|3568|3932|5123|5144|5119|5056|5135|5187|5074|5089|3934|3913|3917|3721|5075|5046|5049|7170|5146|5205|5203|5115|5204|5040|5179|5070|5182|5085|5181|3719|7173|5131|3911|5101|7175|7169|3713|5112|22187|9917|3859|3862|3878|3865|3856|3873|3877|3834|3742|3762|3740|3763|10245|3572|3573|3571|3885|3861 bchest as bitem
        if not timerexists bchestopen
            createtimer bchestopen
            overhead "The treasure is mine!" 2091
        endif
        hotkey 'Grab Item' 
        target bitem
        while queued
            wait 600
        endwhile
        if insysmsg "That container cannot hold"
            overhead "My cup runneth over!..." 1779
            if not targetexists 
                clearall 
            endif
            pause 1200
            break
        endif
    endwhile
endif
loop