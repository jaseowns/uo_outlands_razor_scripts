            @setvar! unholyPoints 13
        elseif ingump "12/" 622436516
            @setvar! unholyPoints 12
        elseif ingump "11/" 622436516
            @setvar! unholyPoints 11
        elseif ingump "10/" 622436516
            @setvar! unholyPoints 10
        elseif ingump "9/" 622436516
